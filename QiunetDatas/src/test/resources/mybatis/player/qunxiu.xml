<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="qunxiu">

    <select id="getQunxiuPo" parameterType="dbInfo" resultType="QunxiuPo">
        select * from ${dbName}.qunxiu where id = #{id};
    </select>

    <update id="updateQunxiuPo" parameterType="dbInfo" >
        update ${dbName}.qunxiu SET
        `name` = #{name},
        `master` = #{master},
        `level` = #{level}
        where id = #{id};
    </update>

    <insert id="insertQunxiuPo" parameterType="dbInfo" >
        insert INTO ${dbName}.qunxiu
        (id, master , `level`, name)
        VALUES (
        #{id}, #{master}, #{level},   #{name});
    </insert>
    <delete id="deleteQunxiuPo" parameterType="dbInfo" >
        DELETE FROM  ${dbName}.qunxiu  WHERE id = #{id};
    </delete>
</mapper>