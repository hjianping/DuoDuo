<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="qunxiu">

    <select id="getQunxiuPo" parameterType="dbInfo" resultType="QunxiuPo">
        select `id`,`name`,`master`,`level` from ${dbName}.qunxiu where id=#{id};
    </select>

    <update id="updateQunxiuPo" parameterType="dbInfo" >
	    update ${dbName}.qunxiu set `name` = #{name} ,`master` = #{master} ,`level` = #{level}  where `id` = #{id};
    </update>

    <insert id="insertQunxiuPo" parameterType="dbInfo" >
	    insert into ${dbName}.qunxiu (`id`,`name`,`master`,`level`) values (#{id} ,#{name} ,#{master} ,#{level} );
    </insert>
	
    <delete id="deleteQunxiuPo" parameterType="dbInfo" >
	    delete from ${dbName}.qunxiu where `id` = #{id};
    </delete>
</mapper>