<?xml version="1.0" encoding="UTF-8"?>
<db_entity key="uid"
		name="PlayerDo"
	    baseDir="src/test/java"
	    comment="玩家存放数据的表"
		packageName="org.qiunet.project.init.test.player"
		xmlns="https://github.com/qiunet/DuoDuo/DbEntity"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="https://github.com/qiunet/DuoDuo/DbEntity"
	>
	<field name="uid" type="long" comment="玩家id" defaultVal="0"/>
	<field name="level" type="int" comment="等级" defaultVal="1"/>
	<field name="gem" type="String" comment="二货币" defaultVal="0"/>
	<field name="m1" type="int" comment="m1" defaultVal="0"/>
	<field name="name" type="String" comment="name" defaultVal="无名"/>

	<constructor>
		<constructor_arg field="uid"/>
	</constructor>
	<constructor>
		<constructor_arg field="uid" />
		<constructor_arg field="level" />
	</constructor>
</db_entity>
